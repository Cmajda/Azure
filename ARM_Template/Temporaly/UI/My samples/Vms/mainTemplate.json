        "dataDisks": {
            "type": "Array",
            "Value": [
                {
                    "lun": 1,
                    "createOption": "attach",
                    "caching": "None",
                    "writeAcceleratorEnabled": false,
                    "id": null,
                    "name": "[variables('datadiskName_1')]",
                    "storageAccountType": null,
                    "diskSizeGB": null,
                    "diskEncryptionSet": null
                },
                {
                    "lun": 2,
                    "createOption": "attach",
                    "caching": "None",
                    "writeAcceleratorEnabled": false,
                    "id": null,
                    "name": "[variables('datadiskName_2')]",
                    "storageAccountType": null,
                    "diskSizeGB": null,
                    "diskEncryptionSet": null
                }
            ]
        },
        "dataDiskResources": {
            "type": "Array",
            "Value": [
                {
                    "name": "[variables('datadiskName_1')]",
                    "sku": "[parameters('DataDiskType_1')]",
                    "properties": {
                        "diskSizeGB": "[parameters('DataDiskSize_1')]",
                        "creationData": {
